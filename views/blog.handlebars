
<main>
    <div class="featureditems">
            <div style="font-size: 50px; color: white ">All Blogs </div> <!--style="overflow-y:scroll"-->
        {{#each blogs as |blog|}}
        <div style="float: left; width: 100%;">
            <div height="50px" class="card row">
                <div class="card-content" style="width:100%">
                    <div class="card-title activator grey-text text-darken-4"> Name: {{blog.title}}</div>
                     <div class="card-title activator grey-text text-darken-4"> Body: {{blog.body}}</div>
                    <div class="card-title activator grey-text text-darken-4"> author: {{blog.author}}</div>
                    {{!-- <div class="card-title activator grey-text text-darken-4"> Time Purchased: {{profit.date}}</div> --}}
                    <div class="right-align" style="valign:top;" >
                    </div>
                    <div class="right-align" style="float: inline-end;">
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</main>
<script src="/js/cart.js"></script>